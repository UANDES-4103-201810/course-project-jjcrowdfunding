<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table>
  <thead>
    <tr>
      <th>Mainpicture</th>
      <th>Video url</th>
      <th>Description</th>
      <th>Tittle</th>
      <th>Goal amount</th>
      <th>Postdate</th>
      <th>Duedate</th>
      <th>Mark down</th>
      <th>Views</th>
      <th>Outstanding value</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.mainpicture %></td>
        <td><%= project.video_url %></td>
        <td><%= project.description %></td>
        <td><%= project.tittle %></td>
        <td><%= project.goal_amount %></td>
        <td><%= project.postdate %></td>
        <td><%= project.duedate %></td>
        <td><%= project.mark_down %></td>
        <td><%= project.views %></td>
        <td><%= project.outstanding_value %></td>
        <td><%= project.category %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
